package JavaB;
import java.lang.Math;

public class Random {
	public static void main(String args[]) {
		System.out.println("1st ramdom Number "+ Math.random());
		
		System.out.println("2st ramdom Number "+ Math.random());
		
		System.out.println("3st ramdom Number "+ Math.random());
		
		System.out.println("4st ramdom Number "+ Math.random());
	}
}
